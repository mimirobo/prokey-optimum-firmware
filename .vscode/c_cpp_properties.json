{
    "configurations": [
        {
            "name": "Prokey-Optimum-Firmware",
            "includePath": [
                "${workspaceRoot}",
                "${workspaceRoot}/",
                "${workspaceRoot}/firmware",
                "${workspaceRoot}/firmware/protob",
                "${workspaceRoot}/vendor/nanopb",
                "${workspaceRoot}/gen",
                "${workspaceRoot}/vendor/libopencm3/include/libopencm3/stm32/f2",
                "${workspaceRoot}/vendor/libopencm3/include/libopencm3/stm32",
                "${workspaceRoot}/vendor/libopencm3/include",
                "${workspaceRoot}/crypto"
            ],
            "intelliSenseMode": "clang-x64",
            "browse": {
                "path": [
                    "${workspaceRoot}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceRoot}/.vscode/browse.vc.db"
            },
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "c++17"
        }
    ],
    "version": 4
}