    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug (OpenOCD USB) - Attach to GDB",
            "type": "gdb",
            "request": "attach",
            "executable": "${workspaceRoot}/firmware/prokey.elf",
            "target": ":3333",
            "remote": true,
            "cwd": "${workspaceRoot}",
            "gdbpath": "arm-none-eabi-gdb.exe",
            "autorun": [
                "monitor reset halt",
                "interrupt",
                //"load ./firmware/prokey.elf 0x08020000",
                //"add-symbol-file ./bootloader/bootloader.elf 0x08010000",
                //"load",
                
             ]
        },
    ]
}